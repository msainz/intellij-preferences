<?xml version="1.0" encoding="UTF-8"?>
<keymap version="1" name="Custom Mac OS X 10.5+" parent="Mac OS X 10.5+">
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="shift alt D" />
  </action>
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="shift alt A" />
  </action>
  <action id="EditorSelectLine">
    <keyboard-shortcut first-keystroke="shift meta L" />
  </action>
  <action id="HideAllWindows">
    <keyboard-shortcut first-keystroke="shift meta F12" />
    <keyboard-shortcut first-keystroke="control meta alt H" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="control meta alt S" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="control meta alt V" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="control alt ENTER" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="control alt DOWN" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="control alt LEFT" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="control alt RIGHT" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="control alt UP" />
  </action>
</keymap>

